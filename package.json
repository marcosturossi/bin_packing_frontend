{
  "name": "bin-packing",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "generate:api1": "openapi-generator-cli generate -i http://localhost:5000/swagger/v1/swagger.json -g typescript-angular -o ./src/app/generated_services/ --additional-properties=ngVersion=21.0.5,supportsES6=true,npmVersion=10.3.0,withInterfaces=true,basePath=http://localhost:5000",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@primeuix/themes": "^2.0.3",
    "keycloak-angular": "^21.0.0",
    "keycloak-js": "^26.2.3",
    "ng-openapi-gen": "^0.52.0",
    "openapi-generator-cli": "^1.0.0",
    "primeng": "^21.1.1",
    "rxjs": "~7.8.0",
    "three": "^0.182.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.5",
    "@angular/cli": "^21.0.5",
    "@angular/compiler-cli": "^21.0.0",
    "@openapitools/openapi-generator-cli": "^2.28.3",
    "@types/three": "^0.182.0",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
